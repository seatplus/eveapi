{
  "name": "seatplus/eveapi",
  "description": "API for receiving informations from esi",
  "license": "MIT",
  "authors": [
    {
      "name": "Felix Huber",
      "email": "felix.a.huber@gmx.net"
    }
  ],
  "autoload": {
    "psr-4": {
      "Seatplus\\Eveapi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Seatplus\\Eveapi\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.8.*",
    "laravel/horizon": "^3.2",
    "eveseat/eseye": "dev-master",
    "spatie/laravel-queueable-action": "^2.1"
  },
  "require-dev": {
    "orchestra/testbench": "^3.8",
    "spatie/phpunit-watcher": "^1.11"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seatplus\\Eveapi\\EveapiServiceProvider"
      ]
    }
  },
  "scripts": {
    "watch": "vendor/bin/phpunit-watcher watch"
  }
}
